 <Method>
  <ItemName>drawCell</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub drawCell(cellX as integer, cellY as integer, drawable as rbrwIterableTreeBand, height as double, width as double, overrunCode as rbrwAdorner.AdornerCellCode=rbrwAdorner.AdornerCellCode.startStop)</SourceLine>
   <SourceLine>dim pt as rbrw.Point</SourceLine>
   <SourceLine>rbrwCurrentDraw.moveTo 0, 0, pt</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// left line</SourceLine>
   <SourceLine>rbrwCurrentDraw.moveTo pt.x, pt.y</SourceLine>
   <SourceLine>dim lineBottom as integer = pt.y + height</SourceLine>
   <SourceLine>rbrwGraphicsLineDE.recordElement pt.x, pt.y, pt.x, lineBottom</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// right line</SourceLine>
   <SourceLine>dim lineRight as integer = pt.x + width</SourceLine>
   <SourceLine>rbrwCurrentDraw.moveTo lineRight, pt.y</SourceLine>
   <SourceLine>rbrwGraphicsLineDE.recordElement lineRight, pt.y, lineRight, lineBottom</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// top line</SourceLine>
   <SourceLine>if overrunCode=rbrwAdorner.AdornerCellCode.startStop or overrunCode=rbrwAdorner.AdornerCellCode.startContinue then</SourceLine>
   <SourceLine>rbrwCurrentDraw.moveTo pt.x, pt.y</SourceLine>
   <SourceLine>rbrwGraphicsLineDE.recordElement pt.x, pt.y, lineRight, pt.y</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// bottom line</SourceLine>
   <SourceLine>if overrunCode = rbrwAdorner.AdornerCellCode.startStop or overrunCode = rbrwAdorner.AdornerCellCode.continueStop then</SourceLine>
   <SourceLine>rbrwCurrentDraw.moveTo pt.x, lineBottom</SourceLine>
   <SourceLine>rbrwGraphicsLineDE.recordElement pt.x, lineBottom, lineRight, lineBottom</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <Shared>0</Shared>
  <ItemParams>cellX as integer, cellY as integer, drawable as rbrwIterableTreeBand, height as double, width as double, overrunCode as rbrwAdorner.AdornerCellCode=rbrwAdorner.AdornerCellCode.startStop</ItemParams>
