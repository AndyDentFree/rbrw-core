  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub operator_convert(rhs as Boolean)</SourceLine>
   <SourceLine>// convenience converter so people can say aBand.border = true</SourceLine>
   <SourceLine>thickness = 1</SourceLine>
   <SourceLine>if rhs then</SourceLine>
   <SourceLine>style = rbrw.lineStyleT.solid</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>style =  rbrw.lineStyleT.none  // in here for symmetry but people could also just choose not to assign a border</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
