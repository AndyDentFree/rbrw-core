 <Method>
  <ItemName>GenerateDescription</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub GenerateDescription()</SourceLine>
   <SourceLine>' TODO make more efficient</SourceLine>
   <SourceLine>mDescription = mTextFont+Str(mTextSize)</SourceLine>
   <SourceLine>if mBold then mDescription = mDescription + " Bold"</SourceLine>
   <SourceLine>if mItalic then mDescription = mDescription + " Italic"</SourceLine>
   <SourceLine>if mUnderline then mDescription = mDescription + " UL"</SourceLine>
   <SourceLine>if mFillColor&lt;&gt;&amp;c000000 then mDescription = mDescription + mFillColor.HTMLString()</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <Shared>0</Shared>
  <ItemParams></ItemParams>
  <ItemResult></ItemResult>
